{
  "devDependencies": {
    "markdown-it-implicit-figures": "^0.12.0",
    "markdown-it-mathjax3": "^4.3.2",
    "nodemon": "^3.1.7",
    "rollup": "^4.24.3",
    "vite-plugin-full-reload": "^1.2.0",
    "vitepress": "^1.4.2"
  },
  "scripts": {
    "docs:dev": "COMMIT_HASH=$(git rev-parse HEAD) nodemon --watch docs/includes --ext md --exec \"vitepress dev docs\"",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:clean": "rm -rf docs/.vitepress/dist docs/.vitepress/cache node_modules",
    "postinstall": "cp -R ./node_modules/mathjax/es5 ./docs/public/mathjax"
  },
  "dependencies": {
    "mathjax": "^3.2.2"
  }
}
